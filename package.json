{
  "name": "react-typescrit-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start:dev": "vite --mode development --host  0.0.0.0",
    "start:prod": "vite --mode production --host 0.0.0.0",
    "start": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "lint": "eslint . --ext .js,.ts,.tsx, .jsx, src",
    "preview": "vite preview",
    "format": "yarn run lint --fix & yarn prettier --write 'src/**/*.{js,jsx,ts,tsx}'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "5.16.0",
    "@mui/material": "5.11.10",
    "@mui/x-data-grid": "7.10.0",
    "axios": "^1.3.2",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.13",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "core-js": "^3.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "react-toastify": "^9.1.2",
    "react-slideshow-image": "4.3.1",
    "react-use": "^17.4.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-basic-ssl": "1.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "prettier": "^2.8.4",
    "sass": "^1.62.0",
    "typescript": "^4.9.3",
    "vite": "^4.3.1"
  }
}
